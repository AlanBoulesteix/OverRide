int	store_number(int *data)
{
	int		ret;

	printf(" Number: ");
	int number = get_unum();
	printf(" Index: ");
	int index = get_unum();
	if (index % 3 == 0)
	{
		puts(" *** ERROR! ***");
		puts("   This index is reserved for wil!");
		puts(" *** ERROR! ***");
		ret = 1;
	}
	else
	{
		*(unsigned int)(index * 4 + data) = number;
		ret = 0;
	}
	return ret;
}

int	read_number(int	*data)
{
	printf(" Index: ");
	int index = get_unum();
	printf(" Number at data[%u] is %u\n", index, data[index]);
	return 0;
}

